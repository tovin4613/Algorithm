    "import sys\n",
    "\n",
    "max_index = 10000000\n",
    "\n",
    "N = int(input())\n",
    "DP = [0] * max_index\n",
    "DP[1] = 0\n",
    "\n",
    "for i in range(2, N+1):\n",
    "    DP[i] = DP[i-1] + 1\n",
    "    if i % 2 == 0:\n",
    "        DP[i] = min(DP[i], DP[i//2] + 1)\n",
    "    if i % 3 == 0:\n",
    "        DP[i] = min(DP[i], DP[i//3] + 1)\n",
    "\n",
    "print(DP[N])\n"
   ]
